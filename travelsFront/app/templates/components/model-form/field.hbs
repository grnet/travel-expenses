{{ component
  componentName

  field=field
  onChange=(action 'updateProperty')
  value=value
  label=label
  errors=errors
  isTouched=(readonly isTouched)
  object=object
}}

{{#if canShowHint }}
  <div class="md-input-hint">
    <div class="message">
        {{ hint }}
    </div>
  </div>
{{/if}}
