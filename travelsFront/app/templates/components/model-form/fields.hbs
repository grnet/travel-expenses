{{#if hasBlock}}
{{else}}
    {{#each fieldsets as |fieldset| }}
        {{model-form/set 
            tagName='' 
            fields=fieldset.fields 
            object=object 
            label=fieldset.label 
            text=fieldset.text
            isTouched=isTouched 
            layout=fieldset.layout
            flat=fieldset.flat
        }}
    {{/each}}
{{/if}}
